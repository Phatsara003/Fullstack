<% include ../partials/header %>

 <br>
 <h2 align = 'center'>เรียงตามตัวอักษร "a-z"</h2>
 <hr>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>E-mail</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody id="myTable">
     <% users.forEach(function(user){ %>
        <tr>
            <td scope="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
        </tr>
      <% }); %>
    </tbody>
</table>






