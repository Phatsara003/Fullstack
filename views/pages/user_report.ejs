<% include ../partials/header %>

<h1 align = 'center'>Top 15 Customers by Sales</h1>
<table class="table">
    <thead>
        <tr>
            <th>Customers ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Total Sales</th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(users) { %>
        <tr>
            <td scope="row"><%= users.users_id %></td>
            <td><%= users.name %></td>
            <td><%= users.email %></td>
            <td>$<%= users.price %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
 <% include ../partials/footer %>




 